{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.4",
        "baum/baum": "@dev",
        "bumbummen99/shoppingcart": "^3.0",
        "czim/laravel-listify": "^2.0",
        "fideloper/proxy": "^4.4",
        "fruitcake/laravel-cors": "^2.0",
        "laravel/framework": "^8.0",
        "laravel/helpers": "^1.3",
        "laravel/ui": "^3.0",
        "laravelcollective/html": "6.2.*",
        "spatie/laravel-medialibrary": "^8",
        "spatie/laravel-sluggable": "^2.5",
        "spatie/laravel-searchable": "^1.7"
    },
    "require-dev": {
        "facade/ignition": "^2.3.6",
        "fzaninotto/faker": "^1.9.1",
        "mockery/mockery": "^1.3.1",
        "nunomaduro/collision": "^5.0",
        "nunomaduro/larastan": "^0.6.4",
        "phpunit/phpunit": "^9.3"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
     "repositories" : [
        {
            "url": "https://github.com/neondigital/eloquence.git",
            "type": "vcs"
        },
        {
            "url": "https://github.com/OmarTaherSaad/LaravelShoppingcart.git",
            "type": "vcs"
        },
        {
            "url": "https://github.com/AmirZandi13/baum.git",
            "type": "vcs"
        }
    ],
    "autoload": {
        "files": [
            "app\/Support\/helpers.php"
        ],
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
